<div id="tableContainer">
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Listen</th>
      </tr>
    </thead>
    <tbody>
      {{#each tracks}}
        <tr>
          <td>{{name}}</td>
          <td>
              <audio controls>
                <source src="{{preview_url}}" type="audio/mpeg">
                <a href="">{{preview_url}}</a>
              </audio>
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>